    "is_dynamic_showform": true,
    "showform_fields": [
        {
            "type": "row"
        },
        {
            "type": "col",
            "class": "col-lg-6"
        },
        {
            "field": "parent_demo_dicts_id",
            "type": "select",
            "label": "Parent DemoDicts Filter",
            "lookup_model": "DemoDicts",
            "filter_for": "parent_id",
            "filter_field": "demo_dicts_id",
            "class_control": "select2 on-refresh",
            "is_option_empty": true,
            "option0_title": "- select to show only Parents with this DemoDicts -",
            "attrs_control": "data-live-search=\"true\""
        },
        {
            "field": "parent_id",
            "label": "Parent",
            "lookup_model": "Demos",
            "lookup_params": "parent",
            "filter_by": "parent_demo_dicts_id",
            "filter_field": "demo_dicts_id",
            "type": "select",
            "is_option0": true,
            "option0_title": "- none -",
            "class_contents": "col-md-3",
            "attrs_control": "data-noautosave=\"true\""
        },
        {
            "field": "demo_dicts_id",
            "label": "DemoDicts",
            "lookup_model": "DemoDicts",
            "type": "select",
            "is_option_empty": true,
            "class_contents": "col-md-6",
            "class_control": "on-refresh",
            "append": [
                {
                    "is_dropdown": true,
                    "class": "btn-secondary",
                    "icon": "bi bi-three-dots-vertical",
                    "label": "more",
                    "hint": "DemoDicts actions",
                    "items": [
                        {
                            "class": "on-fw-modal",
                            "icon": "bi bi-plus",
                            "label": "Add",
                            "hint": "Add DemoDicts",
                            "url": "/Admin/DemoDicts/new?",
                            "attrs": "data-fw-lookup=\"add\""
                        },
                        {
                            "class": "on-fw-modal",
                            "icon": "bi bi-pencil",
                            "label": "Edit",
                            "hint": "Edit selected DemoDicts",
                            "url": "/Admin/DemoDicts/{id}/edit?",
                            "attrs": "data-fw-lookup=\"edit\""
                        },
                        {
                            "is_divider": true
                        },
                        {
                            "icon": "bi bi-gear",
                            "label": "Manage",
                            "hint": "Manage DemoDicts",
                            "url": "/Admin/DemoDicts?"
                        }
                    ]
                }
            ]
        },
        {
            "field": "iname",
            "label": "Title",
            "type": "input",
            "required": true,
            "maxlength": 64
        },
        {
            "field": "idesc",
            "label": "Description",
            "type": "textarea",
            "rows": "5",
            "class_control": "markdown autoresize"
        },
        {
            "field": "idesc2",
            "label": "Wysiwyg editor",
            "type": "textarea",
            "rows": "10",
            "class_control": "fw-html-editor",
            "help_text": "uncomment /common/html_editor in load_script.html"
        },
        {
            "field": "idesc3",
            "label": "Wysiwyg editor short",
            "type": "textarea",
            "rows": "10",
            "class_control": "fw-html-editor-short",
            "help_text": "uncomment /common/html_editor in load_script.html"
        },
        {
            "field": "email",
            "label": "Email (unique field)",
            "type": "email",
            "required": true,
            "validate": "exists isemail",
            "maxlength": 128
        },
        {
            "type": "row",
            "class": "form-row"
        },
        {
            "field": "fint",
            "label": "Integer number",
            "type": "number",
            "min": 0,
            "max": 999999,
            "class": false
        },
        {
            "field": "ffloat",
            "label": "Float number",
            "type": "number",
            "step": 0.1,
            "class": false
        },
        {
            "type": "row_end"
        },

        {
            "field": "fcombo",
            "label": "Combo",
            "type": "select",
            "lookup_tpl": "/common/sel/fcombo.sel",
            "class_contents": "col-md-3"
        },
        {
            "field": "fradio",
            "label": "Radio",
            "type": "radio",
            "lookup_tpl": "/common/sel/fcombo.sel",
            "is_inline": true,
            "class_contents": "col d-flex align-items-center"
        },
        {
            "field": "fyesno",
            "label": "Yes or No",
            "type": "yesno",
            "is_inline": true,
            "class_contents": "col d-flex align-items-center"
        },
        {
            "field": "is_checkbox",
            "label": "Checkbox",
            "type": "cb",
            "class_contents": "col d-flex align-items-center"
        },
        {
            "type": "fieldset",
            "label": "Dates",
            "class": "mb-3"
        },
        {
            "field": "fdate_combo",
            "label": "Date combo",
            "type": "date_combo"
        },
        {
            "field": "fdate_pop",
            "label": "Date popup",
            "type": "date_popup",
            "class_contents": "col-md-3"
        },
        {
            "field": "fdatetime",
            "label": "Date and Time",
            "type": "datetime_popup",
            "default_time": "now",
            "class_contents": "col-md-5"
        },
        {
            "field": "ftime",
            "label": "Time",
            "type": "time",
            "conv": "time_from_seconds"
        },
        {
            "type": "fieldset_end"
        },
        {
            "type": "col_end"
        },
        {
            "type": "col",
            "class": "col-lg-6"
        },
        {
            "field": "id",
            "label": "ID",
            "type": "id"
        },
        {
            "field": "dict_link_auto_id",
            "label": "DemoDicts Autocomplete",
            "type": "autocomplete",
            "autocomplete_url": "/Admin/Demos/(Autocomplete)?q=",
            "lookup_model": "DemoDicts",
            "lookup_field": "iname",
            "help_text": "will add new items to DemoDicts"
        },
        {
            "field": "dict_link_multi",
            "label": "DemoDicts Multi",
            "type": "multicb",
            "lookup_model": "DemoDicts",
            "lookup_params": "some optional params"
        },
        {
            "field": "demo_dicts_link",
            "label": "DemoDicts via Junction Table",
            "type": "multicb",
            "model": "DemosDemoDicts"
        },
        {
            "field": "demos_items",
            "label": "Subtable",
            "class_label": "col-12 text-center hr-header fs-5",
            "type": "subtable_edit",
            "model": "DemosItems",
            "related_field_name": "demos_id",
            "save_fields": "demo_dicts_id iname idesc",
            "save_fields_checkboxes": "is_checkbox|0",
            "required_fields": "demo_dicts_id iname"
        },
        {
            "field": "att_id",
            "label": "Attachment",
            "type": "att_edit",
            "att_category": "general"
        },
        {
            "field": "_att_links",
            "label": "Multiple Attachments",
            "type": "att_links_edit"
        },
        {
            "field": "_att_files",
            "label": "Files Upload",
            "type": "att_files_edit",
            "multiple": true,
            "att_category": "general",
            "att_post_prefix": "att_files1"
        },
        {
            "field": "status",
            "label": "Status",
            "type": "select",
            "lookup_tpl": "/common/sel/status.sel",
            "class_contents": "col-md-3"
        },
        {
            "field": "add_time",
            "label": "Added on",
            "type": "added"
        },
        {
            "field": "upd_time",
            "label": "Updated on",
            "type": "updated"
        },
        {
            "is_custom": true,
            "field": "some_custom_field"
        },
        {
            "type": "col_end"
        },
        {
            "type": "row_end"
        }
